    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> ans1;
        int n=numRows;
        for(int i=1;i<=n;i++){
            long long ans=1;
            vector<int> ansRow;
            ansRow.push_back(1);
            for(int col=1;col<i;col++){
                ans=ans*(i-col);
                ans=ans/(col);
                ansRow.push_back(ans);
            }
            ans1.push_back(ansRow);
        }
        return ans1;
    }
