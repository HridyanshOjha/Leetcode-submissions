 int removeDuplicates(vector<int>& nums) {
        int pre=nums[0];
        int k=1;
        int cnt=1;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==pre) continue;
            else {
                nums[k]=nums[i];
                pre=nums[i];
                k++;
                cnt++;
            }
        }
        return cnt;
    }
